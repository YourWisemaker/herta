{
  "name": "herta",
  "version": "1.0.0",
  "description": "Advanced mathematics framework for scientific, engineering, and financial applications",
  "main": "src/index.js",
  "bin": {
    "herta": "./bin/herta.js"
  },
  "scripts": {
    "test": "jest",
    "test:mocha": "mocha test/**/*.test.js",
    "test:coverage": "nyc --reporter=html --reporter=text mocha test/**/*.test.js",
    "build": "webpack --mode production",
    "prepublishOnly": "npm run test && npm run build",
    "postinstall": "chmod +x ./bin/herta.js",
    "docs": "jsdoc -c jsdoc.json",
    "lint": "eslint src/**/*.js"
  },
  "keywords": [
    "math",
    "mathematics",
    "symbolic",
    "algebra",
    "calculus",
    "differential equations",
    "tensor",
    "automatic differentiation"
  ],
  "author": "Herta Math Team",
  "license": "MIT",
  "homepage": "https://github.com/hertajs/herta",
  "repository": {
    "type": "git",
    "url": "https://github.com/hertajs/herta.git"
  },
  "bugs": {
    "url": "https://github.com/hertajs/herta/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "src",
    "bin",
    "commands",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "complex.js": "^2.1.1",
    "d3": "^7.8.5",
    "decimal.js": "^10.5.0",
    "fraction.js": "^4.2.0",
    "mathjs": "^11.11.1",
    "mocha": "^10.2.0",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2",
    "numeric": "^1.2.6",
    "nyc": "^15.1.0",
    "polynomial": "^1.4.5",
    "seedrandom": "^3.0.5",
    "sharp": "^0.32.6",
    "string-similarity": "^4.0.4",
    "tensor-contraction": "^1.0.4",
    "typed-function": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "babel-loader": "^9.1.2",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.2",
    "jsdoc-template": "^1.2.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}
